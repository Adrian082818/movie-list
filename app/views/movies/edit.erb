<h1>Edit List</h1>

<form method="post" action="/movies/<%=@movies.id%>">
<input type="hidden" id="hidden" name="_method" value="patch">
<label for="title">Title: </label>
<input type="text" name="title" value="<%=@movies.title%>">
<label for="director">Director: </label>
<input type="text" name="director" value="<%=@movies.director%>">
<label for="rating">Rating: </label>
<input type="text" name="rating" value="<%=@movies.rating%>">
<label for="release_year">Release Year: </label>
<input type="text" name="release_year" value="<%=@movies.release_year%>">
<input type="submit">
</form>