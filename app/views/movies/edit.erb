<h1>Edit List</h1>

<form method="POST" action="/movies/<%=@movies.id%>">
<input type="hidden" id="hidden" name="_method" value="PATCH">
<div>
<label for="title">Title: </label>
<input type="text" name="title" value="<%=@movies.title%>">
</div>
<div>
<label for="director">Director: </label>
<input type="text" name="director" value="<%=@movies.director%>">
</div>
<div>
<label for="rating">Rating: </label>
<input type="text" name="rating" value="<%=@movies.rating%>">
</div>
<div>
<label for="genre">Genre: </label>
<input type="text" name="genre" value="<%=@movies.genre%>">
</div>
<div>
<label for="release_year">Release Year: </label>
<input type="text" name="release_year" value="<%=@movies.release_year%>">
</div>
<input type="submit" value="edit">
</form>